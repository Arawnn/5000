{
    "name": "TheCinqMille",
    "description": "game of the year",
    "require": {
        "php": ">=7.2.0",
        "illuminate/database": "5.5.*",
        "monolog/monolog": "^2.0",
        "php-di/php-di": "^6.0",
        "psr/http-factory": "^1.0",
        "respect/validation": "^1.1",
        "slim/slim": "3.*",
        "slim/twig-view": "2.*"
    },
    "autoload": {
        "psr-4": {
            "The5000\\": "src"
        }
